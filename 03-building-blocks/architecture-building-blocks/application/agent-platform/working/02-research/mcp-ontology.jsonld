  "@graph": [
    {
      "@id": "mcp:ModelContextProtocolOntology",
      "@type": "owl:Ontology",
      "dcterms:title": "Model Context Protocol (MCP) Ontology",
      "dcterms:description": "Formal ontology defining the conceptual framework for the Model Context Protocol - an open standard for connecting AI systems with external data sources and tools",
      "dcterms:created": "2025-08-30",
      "dcterms:creator": "Claude (Anthropic)",
      "owl:versionInfo": "1.0",
      "rdfs:seeAlso": "https://modelcontextprotocol.io/specification/"
    },
    {
      "@id": "mcp:MCPHost",
      "@type": "owl:Class",
      "rdfs:label": "MCP Host",
      "rdfs:comment": "The primary application that coordinates AI model interactions through MCP clients and manages the overall system",
      "rdfs:subClassOf": "mcp:MCPComponent"
    },
    {
      "@id": "mcp:MCPClient",
      "@type": "owl:Class",
      "rdfs:label": "MCP Client",
      "rdfs:comment": "Protocol client that maintains a 1:1 connection with MCP servers and handles session management",
      "rdfs:subClassOf": "mcp:MCPComponent"
    },
    {
      "@id": "mcp:MCPServer",
      "@type": "owl:Class",
      "rdfs:label": "MCP Server",
      "rdfs:comment": "External program that exposes tools, resources, and prompts via standardized API to AI models",
      "rdfs:subClassOf": "mcp:MCPComponent"
    },
    {
      "@id": "mcp:MCPComponent",
      "@type": "owl:Class",
      "rdfs:label": "MCP Component",
      "rdfs:comment": "Base class for all Model Context Protocol architectural components"
    },
    {
      "@id": "mcp:MCPTool",
      "@type": "owl:Class",
      "rdfs:label": "MCP Tool",
      "rdfs:comment": "Model-controlled functions that AI can invoke to perform specific actions like querying databases or calling APIs",
      "rdfs:subClassOf": "mcp:MCPPrimitive"
    },
    {
      "@id": "mcp:ReadOnlyTool",
      "@type": "owl:Class",
      "rdfs:label": "Read-Only Tool",
      "rdfs:comment": "Tools that retrieve information without modifying external state",
      "rdfs:subClassOf": "mcp:MCPTool"
    },
    {
      "@id": "mcp:WriteableTool",
      "@type": "owl:Class",
      "rdfs:label": "Writeable Tool",
      "rdfs:comment": "Tools that can modify external systems or data",
      "rdfs:subClassOf": "mcp:MCPTool"
    },
    {
      "@id": "mcp:DestructiveTool",
      "@type": "owl:Class",
      "rdfs:label": "Destructive Tool",
      "rdfs:comment": "Tools that perform irreversible operations requiring special confirmation",
      "rdfs:subClassOf": "mcp:WriteableTool"
    },
    {
