@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix schema: <http://schema.org/> .
@prefix agentic: <https://patternode.com/agentic/> .

# ========================================
# ONTOLOGY METADATA
# ========================================

agentic: a owl:Ontology ;
    dcterms:title "Agentic AI Ontology" ;
    dcterms:description "A comprehensive ontology for describing agentic AI frameworks, platforms, architectural paradigms, and their relationships" ;
    dcterms:creator "Patternode Standards" ;
    dcterms:created "2024-01-15"^^xsd:date ;
    dcterms:modified "2024-01-15"^^xsd:date ;
    owl:versionInfo "1.0.0" ;
    dcterms:license <https://creativecommons.org/licenses/by/4.0/> ;
    rdfs:comment "This ontology defines the semantic vocabulary for the agentic AI ecosystem, including frameworks, platforms, architectural paradigms, protocols, and delivery models." .

# ========================================
# CORE CLASSES
# ========================================

## Architectural Paradigms
agentic:ArchitecturalParadigm a owl:Class ;
    rdfs:label "Architectural Paradigm" ;
    rdfs:comment "A fundamental approach to organizing and coordinating agentic AI workflows" ;
    rdfs:subClassOf schema:Intangible .

agentic:ChainBasedArchitecture a owl:Class ;
    rdfs:label "Chain-Based Architecture" ;
    rdfs:comment "Sequential flow of linked operations with modular components" ;
    rdfs:subClassOf agentic:ArchitecturalParadigm .

agentic:GraphBasedArchitecture a owl:Class ;
    rdfs:label "Graph-Based Architecture" ;
    rdfs:comment "Stateful directed graphs with explicit workflow control" ;
    rdfs:subClassOf agentic:ArchitecturalParadigm .

agentic:ConversationalArchitecture a owl:Class ;
    rdfs:label "Conversational Architecture" ;
    rdfs:comment "Asynchronous, event-driven agent conversations" ;
    rdfs:subClassOf agentic:ArchitecturalParadigm .

agentic:RoleBasedArchitecture a owl:Class ;
    rdfs:label "Role-Based Architecture" ;
    rdfs:comment "Specialized team members with defined roles and responsibilities" ;
    rdfs:subClassOf agentic:ArchitecturalParadigm .

agentic:ModelFirstArchitecture a owl:Class ;
    rdfs:label "Model-First Architecture" ;
    rdfs:comment "Production-focused with LLM-driven autonomous reasoning" ;
    rdfs:subClassOf agentic:ArchitecturalParadigm .

agentic:LightweightArchitecture a owl:Class ;
    rdfs:label "Lightweight Architecture" ;
    rdfs:comment "Minimal primitives with explicit handoffs" ;
    rdfs:subClassOf agentic:ArchitecturalParadigm .

agentic:EnterpriseArchitecture a owl:Class ;
    rdfs:label "Enterprise Architecture" ;
    rdfs:comment "Plugin-based with enterprise integration focus" ;
    rdfs:subClassOf agentic:ArchitecturalParadigm .

agentic:DataNativeArchitecture a owl:Class ;
    rdfs:label "Data-Native Architecture" ;
    rdfs:comment "Agents operating directly within data platforms" ;
    rdfs:subClassOf agentic:ArchitecturalParadigm .

agentic:ConsultingLedArchitecture a owl:Class ;
    rdfs:label "Consulting-Led Architecture" ;
    rdfs:comment "Enterprise-grade with comprehensive lifecycle management" ;
    rdfs:subClassOf agentic:ArchitecturalParadigm .

## Frameworks and Platforms
agentic:AgenticFramework a owl:Class ;
    rdfs:label "Agentic Framework" ;
    rdfs:comment "A software development framework for building agentic AI applications" ;
    rdfs:subClassOf schema:SoftwareApplication .

agentic:AgenticPlatform a owl:Class ;
    rdfs:label "Agentic Platform" ;
    rdfs:comment "A cloud-based or managed platform for deploying and running agentic AI applications" ;
    rdfs:subClassOf schema:SoftwareApplication .

## Protocols
agentic:CommunicationProtocol a owl:Class ;
    rdfs:label "Communication Protocol" ;
    rdfs:comment "A standardized protocol for agent-to-agent or agent-to-tool communication" ;
    rdfs:subClassOf schema:Protocol .

agentic:ModelContextProtocol a owl:Class ;
    rdfs:label "Model Context Protocol" ;
    rdfs:comment "Protocol for standardized tool integration interfaces" ;
    rdfs:subClassOf agentic:CommunicationProtocol .

agentic:AgentToAgentProtocol a owl:Class ;
    rdfs:label "Agent-to-Agent Protocol" ;
    rdfs:comment "Protocol for direct agent-to-agent communication" ;
    rdfs:subClassOf agentic:CommunicationProtocol .

## Delivery Models
agentic:DeliveryModel a owl:Class ;
    rdfs:label "Delivery Model" ;
    rdfs:comment "A model describing how agentic AI systems are delivered and supported" ;
    rdfs:subClassOf schema:Intangible .

agentic:OpenSourceDelivery a owl:Class ;
    rdfs:label "Open Source Delivery" ;
    rdfs:comment "Community-driven development with public source code access" ;
    rdfs:subClassOf agentic:DeliveryModel .

agentic:ClosedSourceDelivery a owl:Class ;
    rdfs:label "Closed Source Delivery" ;
    rdfs:comment "Proprietary development with commercial support" ;
    rdfs:subClassOf agentic:DeliveryModel .

agentic:ConsultancySourceDelivery a owl:Class ;
    rdfs:label "Consultancy Source Delivery" ;
    rdfs:comment "Professional services-led implementation and delivery" ;
    rdfs:subClassOf agentic:DeliveryModel .

## Canonical Architectures
agentic:CanonicalArchitecture a owl:Class ;
    rdfs:label "Canonical Architecture" ;
    rdfs:comment "A reference architecture defining universal design principles" ;
    rdfs:subClassOf schema:Intangible .

agentic:xAgentFramework a owl:Class ;
    rdfs:label "xAgentFramework" ;
    rdfs:comment "Canonical framework architecture for agent development" ;
    rdfs:subClassOf agentic:CanonicalArchitecture .

agentic:xAgentPlatform a owl:Class ;
    rdfs:label "xAgentPlatform" ;
    rdfs:comment "Canonical platform architecture for agent deployment" ;
    rdfs:subClassOf agentic:CanonicalArchitecture .

## Components and Capabilities
agentic:FrameworkComponent a owl:Class ;
    rdfs:label "Framework Component" ;
    rdfs:comment "A functional component within an agentic framework" ;
    rdfs:subClassOf schema:SoftwareApplication .

agentic:PlatformComponent a owl:Class ;
    rdfs:label "Platform Component" ;
    rdfs:comment "A service or capability within an agentic platform" ;
    rdfs:subClassOf schema:SoftwareApplication .

agentic:ArchitecturalLayer a owl:Class ;
    rdfs:label "Architectural Layer" ;
    rdfs:comment "A logical layer in a canonical architecture" ;
    rdfs:subClassOf schema:Intangible .

## Compatibility and Integration
agentic:CompatibilityLevel a owl:Class ;
    rdfs:label "Compatibility Level" ;
    rdfs:comment "The degree of integration support between frameworks and platforms" ;
    rdfs:subClassOf schema:Intangible .

agentic:IntegrationPattern a owl:Class ;
    rdfs:label "Integration Pattern" ;
    rdfs:comment "A pattern for integrating frameworks with platforms" ;
    rdfs:subClassOf schema:Intangible .

# ========================================
# OBJECT PROPERTIES
# ========================================

## Implementation and Relationship Properties
agentic:implements a owl:ObjectProperty ;
    rdfs:label "implements" ;
    rdfs:comment "Indicates that a framework implements an architectural paradigm" ;
    rdfs:domain agentic:AgenticFramework ;
    rdfs:range agentic:ArchitecturalParadigm .

agentic:supports a owl:ObjectProperty ;
    rdfs:label "supports" ;
    rdfs:comment "Indicates that a framework or platform supports a protocol" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( agentic:AgenticFramework agentic:AgenticPlatform ) ] ;
    rdfs:range agentic:CommunicationProtocol .

agentic:supportsFramework a owl:ObjectProperty ;
    rdfs:label "supports framework" ;
    rdfs:comment "Indicates that a platform supports a framework" ;
    rdfs:domain agentic:AgenticPlatform ;
    rdfs:range agentic:AgenticFramework .

agentic:hasCompatibilityLevel a owl:ObjectProperty ;
    rdfs:label "has compatibility level" ;
    rdfs:comment "Describes the level of compatibility between a platform and framework" ;
    rdfs:domain agentic:AgenticPlatform ;
    rdfs:range agentic:CompatibilityLevel .

agentic:usesDeliveryModel a owl:ObjectProperty ;
    rdfs:label "uses delivery model" ;
    rdfs:comment "Indicates the delivery model used by a framework or platform" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( agentic:AgenticFramework agentic:AgenticPlatform ) ] ;
    rdfs:range agentic:DeliveryModel .

## Component and Structure Properties
agentic:hasComponent a owl:ObjectProperty ;
    rdfs:label "has component" ;
    rdfs:comment "Indicates that an entity has a specific component" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( agentic:AgenticFramework agentic:AgenticPlatform agentic:CanonicalArchitecture agentic:ArchitecturalLayer ) ] ;
    rdfs:range [ a owl:Class ; owl:unionOf ( agentic:FrameworkComponent agentic:PlatformComponent ) ] .

agentic:hasLayer a owl:ObjectProperty ;
    rdfs:label "has layer" ;
    rdfs:comment "Indicates that a canonical architecture has an architectural layer" ;
    rdfs:domain agentic:CanonicalArchitecture ;
    rdfs:range agentic:ArchitecturalLayer .

agentic:belongsToLayer a owl:ObjectProperty ;
    rdfs:label "belongs to layer" ;
    rdfs:comment "Indicates that a component belongs to an architectural layer" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( agentic:FrameworkComponent agentic:PlatformComponent ) ] ;
    rdfs:range agentic:ArchitecturalLayer .

## Organizational and Vendor Properties
agentic:developedBy a owl:ObjectProperty ;
    rdfs:label "developed by" ;
    rdfs:comment "Indicates the organization that developed a framework or platform" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( agentic:AgenticFramework agentic:AgenticPlatform ) ] ;
    rdfs:range foaf:Organization .

agentic:competsWith a owl:ObjectProperty ;
    rdfs:label "competes with" ;
    rdfs:comment "Indicates competitive relationship between frameworks or platforms" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( agentic:AgenticFramework agentic:AgenticPlatform ) ] ;
    rdfs:range [ a owl:Class ; owl:unionOf ( agentic:AgenticFramework agentic:AgenticPlatform ) ] ;
    a owl:SymmetricProperty .

agentic:complementsWith a owl:ObjectProperty ;
    rdfs:label "complements with" ;
    rdfs:comment "Indicates complementary relationship between frameworks or platforms" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( agentic:AgenticFramework agentic:AgenticPlatform ) ] ;
    rdfs:range [ a owl:Class ; owl:unionOf ( agentic:AgenticFramework agentic:AgenticPlatform ) ] ;
    a owl:SymmetricProperty .

## Evolution and Succession Properties
agentic:evolvesFrom a owl:ObjectProperty ;
    rdfs:label "evolves from" ;
    rdfs:comment "Indicates that a framework evolves from another framework" ;
    rdfs:domain agentic:AgenticFramework ;
    rdfs:range agentic:AgenticFramework .

agentic:supersedes a owl:ObjectProperty ;
    rdfs:label "supersedes" ;
    rdfs:comment "Indicates that a framework supersedes another framework" ;
    rdfs:domain agentic:AgenticFramework ;
    rdfs:range agentic:AgenticFramework .

## Target and Usage Properties
agentic:targetsAudience a owl:ObjectProperty ;
    rdfs:label "targets audience" ;
    rdfs:comment "Indicates the target audience for a framework or platform" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( agentic:AgenticFramework agentic:AgenticPlatform ) ] ;
    rdfs:range agentic:TargetAudience .

agentic:suitableForUseCase a owl:ObjectProperty ;
    rdfs:label "suitable for use case" ;
    rdfs:comment "Indicates use cases suitable for a framework or platform" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( agentic:AgenticFramework agentic:AgenticPlatform ) ] ;
    rdfs:range agentic:UseCase .

# ========================================
# DATATYPE PROPERTIES
# ========================================

