# ========================================
# ADDITIONAL CONSTRAINTS AND RULES
# ========================================

# Each framework should implement exactly one primary architectural paradigm
[] a owl:Restriction ;
    owl:onProperty agentic:implements ;
    owl:onClass agentic:ArchitecturalParadigm ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    rdfs:comment "Each framework implements exactly one primary architectural paradigm" .

# Platforms must support at least one framework
[] a owl:Restriction ;
    owl:onProperty agentic:supportsFramework ;
    owl:onClass agentic:AgenticFramework ;
    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    rdfs:comment "Platforms must support at least one framework" .

# Framework and platform must have a creator
[] a owl:Restriction ;
    owl:onProperty agentic:developedBy ;
    owl:onClass foaf:Organization ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    rdfs:comment "Each framework and platform must have exactly one developing organization" .
