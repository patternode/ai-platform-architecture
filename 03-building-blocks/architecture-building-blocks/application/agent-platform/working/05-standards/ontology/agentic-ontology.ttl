    rdfs:domain agentic:CompatibilityLevel ;
    rdfs:range xsd:string .

agentic:compatibilityDescription a owl:DatatypeProperty ;
    rdfs:label "compatibility description" ;
    rdfs:comment "Description of what the compatibility level means" ;
    rdfs:domain agentic:CompatibilityLevel ;
    rdfs:range xsd:string .

## Delivery Model Properties
agentic:characteristic a owl:DatatypeProperty ;
    rdfs:label "characteristic" ;
    rdfs:comment "A characteristic of a delivery model" ;
    rdfs:domain agentic:DeliveryModel ;
    rdfs:range xsd:string .

agentic:challenge a owl:DatatypeProperty ;
    rdfs:label "challenge" ;
    rdfs:comment "A challenge associated with a delivery model" ;
    rdfs:domain agentic:DeliveryModel ;
    rdfs:range xsd:string .

# ========================================
# COMPATIBILITY LEVEL INDIVIDUALS
# ========================================

agentic:FullCompatibility a agentic:CompatibilityLevel ;
    rdfs:label "Full Compatibility" ;
    agentic:compatibilityLevel "Full" ;
    agentic:compatibilityDescription "Complete, production-ready integration with comprehensive feature support" .

agentic:NativeCompatibility a agentic:CompatibilityLevel ;
    rdfs:label "Native Compatibility" ;
    agentic:compatibilityLevel "Native" ;
    agentic:compatibilityDescription "Framework specifically designed for or developed by the platform provider" .

agentic:PrimaryCompatibility a agentic:CompatibilityLevel ;
    rdfs:label "Primary Compatibility" ;
    agentic:compatibilityLevel "Primary" ;
    agentic:compatibilityDescription "Platform's preferred or recommended framework integration" .

agentic:StandardCompatibility a agentic:CompatibilityLevel ;
    rdfs:label "Standard Compatibility" ;
    agentic:compatibilityLevel "Compatible" ;
    agentic:compatibilityDescription "Works well through standard APIs and integration patterns" .

agentic:LimitedCompatibility a agentic:CompatibilityLevel ;
    rdfs:label "Limited Compatibility" ;
    agentic:compatibilityLevel "Limited" ;
    agentic:compatibilityDescription "Partial support with some restrictions or limitations" .

agentic:BridgeCompatibility a agentic:CompatibilityLevel ;
    rdfs:label "Bridge Compatibility" ;
    agentic:compatibilityLevel "Bridge" ;
    agentic:compatibilityDescription "Integration possible through intermediate layers or third-party connectors" .

agentic:AdapterCompatibility a agentic:CompatibilityLevel ;
    rdfs:label "Adapter Compatibility" ;
    agentic:compatibilityLevel "Adapter" ;
    agentic:compatibilityDescription "Custom integration layer required to connect framework with platform" .

agentic:ExternalCompatibility a agentic:CompatibilityLevel ;
    rdfs:label "External Compatibility" ;
    agentic:compatibilityLevel "External" ;
    agentic:compatibilityDescription "Integration occurs through external APIs or services" .

agentic:ExperimentalCompatibility a agentic:CompatibilityLevel ;
    rdfs:label "Experimental Compatibility" ;
    agentic:compatibilityLevel "Experimental" ;
    agentic:compatibilityDescription "Early-stage integration with limited production support" .

agentic:CustomCompatibility a agentic:CompatibilityLevel ;
    rdfs:label "Custom Compatibility" ;
    agentic:compatibilityLevel "Custom" ;
    agentic:compatibilityDescription "Integration requires custom development work" .

agentic:ConsultingCompatibility a agentic:CompatibilityLevel ;
    rdfs:label "Consulting Compatibility" ;
    agentic:compatibilityLevel "Consulting" ;
    agentic:compatibilityDescription "Integration available through professional services" .

agentic:APIOnlyCompatibility a agentic:CompatibilityLevel ;
    rdfs:label "API-Only Compatibility" ;
    agentic:compatibilityLevel "API-only" ;
    agentic:compatibilityDescription "Integration limited to REST API or similar external interfaces" .

agentic:DataSharingCompatibility a agentic:CompatibilityLevel ;
    rdfs:label "Data Sharing Compatibility" ;
    agentic:compatibilityLevel "Data sharing" ;
    agentic:compatibilityDescription "Integration primarily focused on data exchange" .

agentic:MultiCloudCompatibility a agentic:CompatibilityLevel ;
    rdfs:label "Multi-Cloud Compatibility" ;
    agentic:compatibilityLevel "Multi-cloud" ;
    agentic:compatibilityDescription "Framework supports platform as part of multi-cloud strategy" .

agentic:NoCompatibility a agentic:CompatibilityLevel ;
    rdfs:label "No Compatibility" ;
    agentic:compatibilityLevel "None" ;
    agentic:compatibilityDescription "No integration support available" .

