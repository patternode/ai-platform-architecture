    rdfs:domain agentic:ArchitecturalParadigm ;
    rdfs:range xsd:string .

agentic:coordinationModel a owl:DatatypeProperty ;
    rdfs:label "coordination model" ;
    rdfs:comment "The coordination model used by an architectural paradigm" ;
    rdfs:domain agentic:ArchitecturalParadigm ;
    rdfs:range xsd:string .

agentic:stateManagement a owl:DatatypeProperty ;
    rdfs:label "state management" ;
    rdfs:comment "The state management approach of an architectural paradigm" ;
    rdfs:domain agentic:ArchitecturalParadigm ;
    rdfs:range xsd:string .

agentic:complexityHandling a owl:DatatypeProperty ;
    rdfs:label "complexity handling" ;
    rdfs:comment "How an architectural paradigm handles complexity" ;
    rdfs:domain agentic:ArchitecturalParadigm ;
    rdfs:range xsd:string .

## Framework and Platform Properties
agentic:multiAgentSupport a owl:DatatypeProperty ;
    rdfs:label "multi-agent support" ;
    rdfs:comment "The level of multi-agent support provided" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( agentic:AgenticFramework agentic:AgenticPlatform ) ] ;
    rdfs:range xsd:string .

agentic:memoryManagement a owl:DatatypeProperty ;
    rdfs:label "memory management" ;
    rdfs:comment "The memory management capabilities" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( agentic:AgenticFramework agentic:AgenticPlatform ) ] ;
    rdfs:range xsd:string .

agentic:productionReadiness a owl:DatatypeProperty ;
    rdfs:label "production readiness" ;
    rdfs:comment "Whether the framework or platform is production-ready" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( agentic:AgenticFramework agentic:AgenticPlatform ) ] ;
    rdfs:range xsd:boolean .

agentic:primaryFocus a owl:DatatypeProperty ;
    rdfs:label "primary focus" ;
    rdfs:comment "The primary focus area of a platform" ;
    rdfs:domain agentic:AgenticPlatform ;
    rdfs:range xsd:string .

agentic:coreIntegration a owl:DatatypeProperty ;
    rdfs:label "core integration" ;
    rdfs:comment "The core integration approach of a platform" ;
    rdfs:domain agentic:AgenticPlatform ;
    rdfs:range xsd:string .

agentic:securityModel a owl:DatatypeProperty ;
    rdfs:label "security model" ;
    rdfs:comment "The security model employed by a platform" ;
    rdfs:domain agentic:AgenticPlatform ;
    rdfs:range xsd:string .

agentic:targetAudience a owl:DatatypeProperty ;
    rdfs:label "target audience" ;
    rdfs:comment "The intended target audience" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( agentic:AgenticFramework agentic:AgenticPlatform ) ] ;
    rdfs:range xsd:string .

## Programming and Technical Properties
agentic:programmingLanguage a owl:DatatypeProperty ;
    rdfs:label "programming language" ;
    rdfs:comment "Programming languages supported by the framework" ;
    rdfs:domain agentic:AgenticFramework ;
    rdfs:range xsd:string .

agentic:operatingSystem a owl:DatatypeProperty ;
    rdfs:label "operating system" ;
    rdfs:comment "Operating system requirements or preferences" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( agentic:AgenticFramework agentic:AgenticPlatform ) ] ;
    rdfs:range xsd:string .

agentic:deploymentModel a owl:DatatypeProperty ;
    rdfs:label "deployment model" ;
    rdfs:comment "Available deployment models" ;
    rdfs:domain agentic:AgenticPlatform ;
    rdfs:range xsd:string .

## Quality and Assessment Properties
agentic:enterpriseConsiderations a owl:DatatypeProperty ;
    rdfs:label "enterprise considerations" ;
    rdfs:comment "Important considerations for enterprise adoption" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( agentic:AgenticFramework agentic:AgenticPlatform ) ] ;
    rdfs:range xsd:string .

agentic:strength a owl:DatatypeProperty ;
    rdfs:label "strength" ;
    rdfs:comment "A key strength or advantage" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( agentic:AgenticFramework agentic:AgenticPlatform ) ] ;
    rdfs:range xsd:string .

agentic:limitation a owl:DatatypeProperty ;
    rdfs:label "limitation" ;
    rdfs:comment "A limitation or constraint" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( agentic:AgenticFramework agentic:AgenticPlatform ) ] ;
    rdfs:range xsd:string .

agentic:bestUseCase a owl:DatatypeProperty ;
    rdfs:label "best use case" ;
