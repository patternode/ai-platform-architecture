        </mxCell>
        <mxCell id="cap2-sub4" value="&lt;b&gt;&lt;font color=&quot;#002F6B&quot;&gt;2.4 Model Monitoring &amp; Observability&lt;/font&gt;&lt;/b&gt;&#xa;&#xa;&lt;font&gt;Continuous monitoring systems tracking model performance, data drift, concept drift, and operational metrics in production with real-time alerting and automated anomaly detection&lt;/font&gt;" style="rounded=6;whiteSpace=wrap;html=1;fillColor=#FFFFFF;strokeColor=#002F6B;strokeWidth=2;fontSize=14;fontColor=#333333;fontFamily=Arial;align=left;spacingLeft=8;spacingTop=5;verticalAlign=top;" parent="1" vertex="1">
          <mxGeometry x="-2910" y="-772" width="360" height="140.5" as="geometry" />
