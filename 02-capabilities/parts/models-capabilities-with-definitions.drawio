        <mxCell id="cap2-sub3" value="&lt;b&gt;&lt;font color=&quot;#002F6B&quot;&gt;2.3 Model Deployment &amp; Release&lt;/font&gt;&lt;/b&gt;&#xa;&#xa;&lt;font&gt;Automated deployment pipelines supporting containerization, orchestration, and multi-environment deployment strategies including canary releases, blue-green deployments, and automated rollback&lt;/font&gt;" style="rounded=6;whiteSpace=wrap;html=1;fillColor=#FFFFFF;strokeColor=#002F6B;strokeWidth=2;fontSize=14;fontColor=#333333;fontFamily=Arial;align=left;spacingLeft=8;spacingTop=5;verticalAlign=top;" parent="1" vertex="1">
