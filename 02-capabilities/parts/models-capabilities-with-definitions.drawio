        <mxCell id="cap3-sub1" value="&lt;b&gt;&lt;font color=&quot;#002F6B&quot;&gt;3.1 Model Access &amp; Inference&lt;/font&gt;&lt;/b&gt;&#xa;&#xa;&lt;font&gt;Secure, scalable access management for model inference through standardized APIs and endpoints with authentication, authorization, rate limiting, and comprehensive usage tracking&lt;/font&gt;" style="rounded=6;whiteSpace=wrap;html=1;fillColor=#FFFFFF;strokeColor=#002F6B;strokeWidth=2;fontSize=14;fontColor=#333333;fontFamily=Arial;align=left;spacingLeft=8;spacingTop=5;verticalAlign=top;" parent="1" vertex="1">
