        <mxCell id="cap3-sub9" value="&lt;b&gt;&lt;font color=&quot;#002F6B&quot;&gt;3.9 Context Assembly &amp;amp; Orchestration&lt;/font&gt;&lt;/b&gt;&lt;br&gt;&lt;br&gt;&lt;font&gt;Dynamic context assembly, RAG integration, conversation history, multi-source fusion, prompt versioning, template libraries, and token optimization&lt;/font&gt;" style="rounded=6;whiteSpace=wrap;html=1;fillColor=#FFFFFF;strokeColor=#002F6B;strokeWidth=2;fontSize=14;fontColor=#333333;fontFamily=Arial;align=left;spacingLeft=8;spacingTop=5;verticalAlign=top;" parent="1" vertex="1">
