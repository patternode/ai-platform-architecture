        <mxCell id="cap1-sub1" value="&lt;b&gt;&lt;font color=&quot;#002F6B&quot;&gt;1.1 Model Registry &amp; Cataloging&lt;/font&gt;&lt;/b&gt;&#xa;&#xa;&lt;font&gt;Centralized, authoritative repository for cataloging, versioning, and tracking all AI/ML models across the organization with comprehensive metadata management, lineage tracking, and model discovery capabilities&lt;/font&gt;" style="rounded=6;whiteSpace=wrap;html=1;fillColor=#FFFFFF;strokeColor=#002F6B;strokeWidth=2;fontSize=14;fontColor=#333333;fontFamily=Arial;align=left;spacingLeft=8;spacingTop=5;verticalAlign=top;spacingRight=2;" parent="1" vertex="1">
          <mxGeometry x="-3730" y="-932.5" width="260" height="150" as="geometry" />
        </mxCell>
        <mxCell id="cap1-sub2" value="&lt;b&gt;&lt;font color=&quot;#002F6B&quot;&gt;1.2 Model Approval &amp; Gate Management&lt;/font&gt;&lt;/b&gt;&#xa;&#xa;&lt;font&gt;Structured review and approval processes ensuring models meet quality, compliance, business, and technical requirements before progressing through deployment gates from development to production&lt;/font&gt;" style="rounded=6;whiteSpace=wrap;html=1;fillColor=#FFFFFF;strokeColor=#002F6B;strokeWidth=2;fontSize=14;fontColor=#333333;fontFamily=Arial;align=left;spacingLeft=8;spacingTop=5;verticalAlign=top;spacingRight=2;" parent="1" vertex="1">
          <mxGeometry x="-3460" y="-932.5" width="280" height="150" as="geometry" />
        </mxCell>
        <mxCell id="cap1-sub3" value="&lt;b&gt;&lt;font color=&quot;#002F6B&quot;&gt;1.3 Model Compliance &amp; Regulatory Adherence&lt;/font&gt;&lt;/b&gt;&#xa;&#xa;&lt;font&gt;Ensuring all models adhere to banking regulations, internal policies, and audit standards including Basel III, APRA CPS 230, GDPR, and local financial services requirements&lt;/font&gt;" style="rounded=6;whiteSpace=wrap;html=1;fillColor=#FFFFFF;strokeColor=#002F6B;strokeWidth=2;fontSize=14;fontColor=#333333;fontFamily=Arial;align=left;spacingLeft=8;spacingTop=5;verticalAlign=top;" parent="1" vertex="1">
          <mxGeometry x="-3170" y="-932.5" width="250" height="152.5" as="geometry" />
        </mxCell>
        <mxCell id="cap1-sub4" value="&lt;b&gt;&lt;font color=&quot;#002F6B&quot;&gt;1.4 Model Risk Management &amp; Ethics&lt;/font&gt;&lt;/b&gt;&#xa;&#xa;&lt;font&gt;Systematically identify, assess, and mitigate model-related risks including bias, fairness, ethical concerns, and discriminatory patterns while maintaining responsible AI practices&lt;/font&gt;" style="rounded=6;whiteSpace=wrap;html=1;fillColor=#FFFFFF;strokeColor=#002F6B;strokeWidth=2;fontSize=14;fontColor=#333333;fontFamily=Arial;align=left;spacingLeft=8;spacingTop=5;verticalAlign=top;" parent="1" vertex="1">
          <mxGeometry x="-3730" y="-772" width="260" height="140" as="geometry" />
        </mxCell>
        <mxCell id="cap1-sub5" value="&lt;b&gt;&lt;font color=&quot;#002F6B&quot;&gt;1.5 Model Documentation &amp; Lineage&lt;/font&gt;&lt;/b&gt;&#xa;&#xa;&lt;font&gt;Comprehensive documentation standards covering model lineage, metadata, performance characteristics, and complete traceability from raw data to production deployment&lt;/font&gt;" style="rounded=6;whiteSpace=wrap;html=1;fillColor=#FFFFFF;strokeColor=#002F6B;strokeWidth=2;fontSize=14;fontColor=#333333;fontFamily=Arial;align=left;spacingLeft=8;spacingTop=5;verticalAlign=top;" parent="1" vertex="1">
          <mxGeometry x="-3460" y="-772" width="270" height="143" as="geometry" />
        </mxCell>
        <mxCell id="cap1-sub6" value="&lt;b&gt;&lt;font color=&quot;#002F6B&quot;&gt;1.6 Model Types &amp;amp; Classification&lt;/font&gt;&lt;/b&gt;&lt;br&gt;&lt;br&gt;&lt;font&gt;Categorize and manage different model types (Foundation, Fine-Tuned, Custom, Domain-Specific, Embedded) with type-specific governance, lifecycle templates, and cost management&lt;/font&gt;" style="rounded=6;whiteSpace=wrap;html=1;fillColor=#FFFFFF;strokeColor=#002F6B;strokeWidth=2;fontSize=14;fontColor=#333333;fontFamily=Arial;align=left;spacingLeft=8;spacingTop=5;verticalAlign=top;" parent="1" vertex="1">
