        <mxCell id="cap3-sub6" value="&lt;b&gt;&lt;font color=&quot;#002F6B&quot;&gt;3.6 Model Rollback &amp; Recovery&lt;/font&gt;&lt;/b&gt;&#xa;&#xa;&lt;font&gt;Rapid rollback and recovery from deployment issues, performance degradation, or operational problems through automated triggers, version restoration, and disaster recovery procedures&lt;/font&gt;" style="rounded=6;whiteSpace=wrap;html=1;fillColor=#FFFFFF;strokeColor=#002F6B;strokeWidth=2;fontSize=14;fontColor=#333333;fontFamily=Arial;align=left;spacingLeft=8;spacingTop=5;verticalAlign=top;" parent="1" vertex="1">
          <mxGeometry x="-3730" y="-384" width="210" height="198" as="geometry" />
