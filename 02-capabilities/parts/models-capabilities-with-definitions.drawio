        <mxCell id="cap3-header" value="3. MODEL OPERATIONS (MODELOPS)" style="rounded=6;whiteSpace=wrap;html=1;fillColor=light-dark(#5a1e58, #a6ceff);strokeColor=none;fontColor=#FFFFFF;fontStyle=1;fontSize=18;fontFamily=Arial;align=center;" parent="1" vertex="1">
