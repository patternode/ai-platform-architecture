        <mxCell id="cap1-sub1" value="&lt;b&gt;&lt;font color=&quot;#002F6B&quot;&gt;1.1 Runtime Environment&lt;/font&gt;&lt;/b&gt;&#xa;&#xa;&lt;font&gt;Secure, serverless execution environment that provides the foundational compute infrastructure for running AI agents with session isolation, auto-scaling, and long-running task support&lt;/font&gt;" style="rounded=6;whiteSpace=wrap;html=1;fillColor=#FFFFFF;strokeColor=#002F6B;strokeWidth=2;fontSize=14;fontColor=#333333;fontFamily=Arial;align=left;spacingLeft=8;spacingTop=5;verticalAlign=top;spacingRight=2;" parent="1" vertex="1">
          <mxGeometry x="-1800" y="-880" width="210" height="182" as="geometry" />
        </mxCell>
        <mxCell id="cap1-sub2" value="&lt;b&gt;&lt;font color=&quot;#002F6B&quot;&gt;1.2 Session Management&lt;/font&gt;&lt;/b&gt;&#xa;&#xa;&lt;font&gt;Capabilities for managing agent sessions including state persistence, checkpoint recovery, session lifecycle, and context preservation across long-running workflows&lt;/font&gt;" style="rounded=6;whiteSpace=wrap;html=1;fillColor=#FFFFFF;strokeColor=#002F6B;strokeWidth=2;fontSize=14;fontColor=#333333;fontFamily=Arial;align=left;spacingLeft=8;spacingTop=5;verticalAlign=top;spacingRight=2;" parent="1" vertex="1">
          <mxGeometry x="-1580" y="-880" width="200" height="182" as="geometry" />
        </mxCell>
        <mxCell id="cap1-sub3" value="&lt;b&gt;&lt;font color=&quot;#002F6B&quot;&gt;1.3 Identity &amp;amp; Access Management&lt;/font&gt;&lt;/b&gt;&#xa;&#xa;&lt;font&gt;Enterprise-grade identity services providing secure authentication, authorization, impersonation flows, and role-based access control for agents and users with comprehensive audit trails&lt;/font&gt;" style="rounded=6;whiteSpace=wrap;html=1;fillColor=#FFFFFF;strokeColor=#002F6B;strokeWidth=2;fontSize=14;fontColor=#333333;fontFamily=Arial;align=left;spacingLeft=8;spacingTop=5;verticalAlign=top;" parent="1" vertex="1">
          <mxGeometry x="-1370" y="-880" width="200" height="182" as="geometry" />
        </mxCell>
        <mxCell id="cap1-sub4" value="&lt;b&gt;&lt;font color=&quot;#002F6B&quot;&gt;1.4 Gateway Services&lt;/font&gt;&lt;/b&gt;&#xa;&#xa;&lt;font&gt;API gateway and integration services that provide secure, managed connectivity between agents and external services, APIs, and enterprise systems with zero-code transformations and protocol handling&lt;/font&gt;" style="rounded=6;whiteSpace=wrap;html=1;fillColor=#FFFFFF;strokeColor=#002F6B;strokeWidth=2;fontSize=14;fontColor=#333333;fontFamily=Arial;align=left;spacingLeft=8;spacingTop=5;verticalAlign=top;" parent="1" vertex="1">
          <mxGeometry x="-1800" y="-691" width="210" height="162" as="geometry" />
        </mxCell>
        <mxCell id="cap1-sub5" value="&lt;b&gt;&lt;font color=&quot;#002F6B&quot;&gt;1.5 Memory Services&lt;/font&gt;&lt;/b&gt;&#xa;&#xa;&lt;font&gt;Managed memory infrastructure providing short-term and long-term storage for agent conversations, context, experiences, and learned knowledge with efficient retrieval and persistence&lt;/font&gt;" style="rounded=6;whiteSpace=wrap;html=1;fillColor=#FFFFFF;strokeColor=#002F6B;strokeWidth=2;fontSize=14;fontColor=#333333;fontFamily=Arial;align=left;spacingLeft=8;spacingTop=5;verticalAlign=top;" parent="1" vertex="1">
          <mxGeometry x="-1580" y="-691" width="200" height="162" as="geometry" />
        </mxCell>
        <mxCell id="cap1-sub6" value="&lt;b&gt;&lt;font color=&quot;#002F6B&quot;&gt;1.6 Observability Platform&lt;/font&gt;&lt;/b&gt;&lt;br&gt;&lt;br&gt;&lt;font&gt;Monitoring, logging, tracing, and analytics infrastructure for tracking agent performance, behavior, system health, and operational metrics with real-time dashboards and alerting&lt;/font&gt;" style="rounded=6;whiteSpace=wrap;html=1;fillColor=#FFFFFF;strokeColor=#002F6B;strokeWidth=2;fontSize=14;fontColor=#333333;fontFamily=Arial;align=left;spacingLeft=8;spacingTop=5;verticalAlign=top;" parent="1" vertex="1">
